{
  "name": "@money-wise/mobile",
  "version": "0.6.1",
  "description": "MoneyWise React Native Mobile Application",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "dev": "expo start --dev-client",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "lint": "echo '[PLACEHOLDER] Mobile app lint skipped - no code yet' && exit 0",
    "lint:fix": "echo '[PLACEHOLDER] Mobile app lint skipped - no code yet' && exit 0",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:unit": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "clean": "rm -rf .expo android/build ios/build coverage"
  },
  "dependencies": {
    "@money-wise/types": "workspace:*",
    "@money-wise/utils": "workspace:*",
    "expo": "~54.0.30",
    "expo-constants": "~17.0.0",
    "expo-router": "~4.0.0",
    "expo-status-bar": "~2.0.0",
    "nativewind": "^4.1.0",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-safe-area-context": "~4.14.0",
    "react-native-screens": "~4.4.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "~18.3.0",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.6.0"
  }
}
