{
  "name": "@money-wise/backend",
  "version": "0.5.1",
  "description": "MoneyWise NestJS Backend API",
  "private": true,
  "//": "BUILD PROCESS: 'pnpm build' runs: (1) prisma generate â†’ (2) nest build. Prisma Client MUST be generated before TypeScript compilation to avoid import errors. CI/CD workflows depend on this sequence. Pre-commit hooks verify generated/ exists.",
  "scripts": {
    "build": "echo 'ðŸ”§ Generating Prisma Client...' && prisma generate && echo 'âœ… Prisma Client generated' && nest build",
    "dev": "nest start --watch",
    "start": "nest start",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "pretest": "prisma generate",
    "pretest:all": "prisma generate",
    "pretest:unit": "prisma generate",
    "pretest:integration": "prisma generate",
    "test": "jest",
    "test:all": "jest --runInBand",
    "test:unit": "jest --testPathPatterns='__tests__/unit'",
    "test:integration": "jest --testPathPatterns='__tests__/integration'",
    "test:e2e": "jest --config ./__tests__/e2e/jest-e2e.json",
    "test:performance": "jest --testPathPatterns='__tests__/performance' --runInBand --testTimeout=120000 && node scripts/format-benchmarks.js",
    "test:contracts": "jest --testPathPatterns='__tests__/contracts'",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:unit": "jest --coverage --testPathPatterns='__tests__/unit'",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:ci": "pnpm run test:unit && pnpm run test:integration && pnpm run test:contracts",
    "test:ci:integration-only": "jest --testPathPatterns='__tests__/integration' --runInBand",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:reset": "prisma migrate reset",
    "db:generate": "prisma migrate dev --create-only",
    "db:seed": "ts-node src/database/seeds/index.ts",
    "db:seed:auth": "ts-node src/database/seeds/auth-seed.ts",
    "db:seed:demo": "ts-node src/database/seeds/demo-seed.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "prisma:validate": "prisma validate",
    "prisma:format": "prisma format"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.896.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.896.0",
    "@money-wise/types": "workspace:*",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/swagger": "^11.2.3",
    "@prisma/client": "^6.18.0",
    "@sentry/nestjs": "^10.27.0",
    "@sentry/node": "^10.27.0",
    "@sentry/profiling-node": "^10.27.0",
    "@types/compression": "^1.8.1",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "argon2": "^0.44.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "qrcode": "^1.5.4",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "speakeasy": "^2.0.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@testcontainers/postgresql": "^10.4.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "jest-openapi": "^0.14.2",
    "openapi-types": "^12.1.3",
    "prisma": "^6.18.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-parser": "^10.0.3",
    "testcontainers": "^10.4.0",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}